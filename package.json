{
  "name": "javascript-base",
  "version": "1.0.0-alpha.0",
  "description": "javascript base repo include eslint prettier fec-builder stylelint",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint . --ext .js,jsx",
    "lint:style": "stylelint \"**/*.scss\" --syntax scss",
    "format": "npm run lint:es && npm run lint:style",
    "format:es": "npm run lint:es -- --fix",
    "format:style": "npm run lint:style -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doxiaodong/javascript-base.git"
  },
  "author": "duxiaodong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doxiaodong/javascript-base/issues"
  },
  "homepage": "https://github.com/doxiaodong/javascript-base#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.7.3",
    "prettier-stylelint": "^0.4.2",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "stylelint-config-standard": "^17.0.0",
    "typescript": "^2.5.3"
  }
}
