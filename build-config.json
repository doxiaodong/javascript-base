{
  "extends": "react",
  "entries": {
    "index": "src/index.jsx"
  },
  "pages": {
    "index": {
      "template": "src/index.html",
      "entries": "index",
      "path": ""
    }
  },
  "transforms": {
    "scss": {
      "transformer": "scss",
      "config": {
        "autoprefixOptions": {
          "browsers": ["last 2 version", "> 20%"]
        }
      }
    }
  }
}
